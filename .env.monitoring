# Logging Configuration
LOG_LEVEL=debug
ELASTICSEARCH_NODE=http://elasticsearch:9200
ELASTICSEARCH_USER=elastic
ELASTICSEARCH_PASSWORD=changeme

# Sentry Configuration
SENTRY_DSN=https://YOUR_SENTRY_DSN@sentry.io/PROJECT_ID
